{
  "name": "szonn/contao-ajax-reload-element",
  "type": "contao-bundle",
  "description": "SZONN AjaxReloadElement for Contao OpenSource CMS",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Szonn",
      "homepage": "https://szonn.com/"
    }
  ],
  "require": {
    "php": "^5.5 || ^7.0",
    "contao/core-bundle": "^4.4",
    "contao-community-alliance/composer-plugin": "^3.0",
    "contao-community-alliance/url-builder": "^1.2",
    "symfony/http-foundation": "4.4.20"
  },
  "autoload": {
    "psr-4": {
      "Szonn\\ContaoAjaxReloadElementBundle\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "Szonn\\ContaoAjaxReloadElementBundle\\ContaoManager\\Plugin",
    "contao": {
      "sources": {
        "src/Resources/public": "assets/ajax_reload_element"
      }
    }
  },
    "support": {
    "issues": "https://github.com/szonn/contao-ajax-reload-element/blob/main/README.md",
    "source": "https://github.com/szonn/contao-ajax-reload-element/"
  }
}
